{
  "comments": {
    "lineComment": "//",
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"],
    ["<<", ">>"],
    ["{{", "}}"],
    ["[[", "]]"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "<", "close": ">" },
    { "open": "<<", "close": ">>" },
    { "open": "{{", "close": "}}" },
    { "open": "[[", "close": "]]" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"],
    ["<<", ">>"],
    ["{{", "}}"],
    ["[[", "]]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\{\\{",
      "end": "^\\s*\\}\\}"
    }
  },
  "wordPattern": "[\\w_]+(?:\\.[\\w_]+)*(?:\\.[\\w_]+)?"
}


